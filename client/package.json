  "devDependencies": {
    "@types/enzyme": "^3.9.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.13",
    "@types/next": "^7.0.6",
    "@types/react": "^16.4.16",
    "@types/react-dom": "16.0.11",
    "babel-jest": "^24.8.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "fork-ts-checker-webpack-plugin": "^1.3.3",
