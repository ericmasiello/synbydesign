{
  "name": "syn-by-design",
  "version": "3.0.0",
  "description": "Eric Masiello's portfolio",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:watch-server": "npm run build-server -- --watch",
    "predev:watch-client": "npm run clean",
    "dev:watch-client": "webpack --config webpack.client.js --watch",
    "build-server": "webpack --config webpack.server.js",
    "prod:build-server": "npm run build-server -- -p",
    "preprod:build-client": "npm run clean",
    "prod:build-client": "webpack --config webpack.client.js -p",
    "build": "run-s prod:*",
    "start": "node build/bundle.js",
    "lint": "tslint 'src/**/*.ts?(x)' --format stylish -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "clean": "rimraf public; rimraf views",
    "validate": "npm-run-all --parallel test:coverage lint",
    "precommit": "lint-staged",
    "prepush": "npm run validate",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "author": "Eric Masiello",
  "license": "ISC",
  "dependencies": {
    "await-to-js": "~2.0.1",
    "axios": "0.18.0",
    "boom": "~7.1.1",
    "compression": "1.7.2",
    "concurrently": "3.5.1",
    "ejs": "~2.5.7",
    "express": "4.16.2",
    "lodash-es": "~4.17.5",
    "query-string": "~6.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "1.4.0",
    "styled-components": "~3.1.0",
    "tinycolor2": "~1.4.1",
    "winston": "~2.4.0"
  },
  "devDependencies": {
    "@types/axios": "~0.14.0",
    "@types/boom": "~7.1.1",
    "@types/enzyme": "~3.1.9",
    "@types/express": "~4.11.1",
    "@types/jest": "~22.1.3",
    "@types/lodash-es": "~4.17.0",
    "@types/query-string": "~5.1.0",
    "@types/react": "~16.0.38",
    "@types/react-dom": "~16.0.4",
    "@types/react-helmet": "~5.0.4",
    "@types/react-redux": "~5.0.15",
    "@types/react-router-config": "~1.0.6",
    "@types/react-router-dom": "~4.2.4",
    "@types/react-test-renderer": "~16.0.1",
    "@types/redux": "~3.6.31",
    "@types/redux-thunk": "~2.1.32",
    "@types/serialize-javascript": "~1.3.2",
    "@types/tinycolor2": "~1.4.0",
    "@types/winston": "~2.3.8",
    "awesome-typescript-loader": "~4.0.1",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "file-loader": "~1.1.8",
    "html-webpack-plugin": "~2.30.1",
    "husky": "~0.14.3",
    "image-webpack-loader": "~4.1.0",
    "jest": "~22.4.0",
    "jest-styled-components": "~4.10.0",
    "lint-staged": "~7.0.0",
    "nodemon": "1.15.1",
    "npm-run-all": "4.1.2",
    "prettier": "~1.10.2",
    "raw-loader": "~0.5.1",
    "react-test-renderer": "~16.2.0",
    "rimraf": "~2.6.2",
    "svg-inline-loader": "~0.8.0",
    "ts-jest": "~22.0.4",
    "tslint": "~5.9.1",
    "tslint-config-airbnb": "~5.7.0",
    "tslint-config-prettier": "~1.8.0",
    "tslint-loader": "~3.5.3",
    "tslint-react": "~3.5.1",
    "typescript": "~2.8.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "~4.1.1",
    "webpack-node-externals": "1.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -p tsconfig.json --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/src/typings.d.ts",
      "/src/global.d.ts",
      "/src/types.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 40,
        "statements": 40
      }
    },
    "setupFiles": [
      "./test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/test/empty-module.js"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/public",
      "<rootDir>/build"
    ]
  }
}
