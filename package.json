{
  "name": "syn-by-design",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "node": "6.9.4"
  },
  "scripts": {
    "build:svg": "svgo --pretty --indent=2 --folder=artwork/ --config=.svgo.yml && gulp",
    "build:ui": "webpack",
    "watch:ui": "npm run build:ui && webpack --watch",
    "build:server": "babel --out-dir build src",
    "watch:server": "nodemon ./src",
    "prestart": "npm run build:server",
    "start": "cross-env NODE_ENV=production node ./build",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel watch:server watch:ui",
    "lint": "eslint . .eslintrc.js --ext [js,jsx] --cache",
    "jsdoc": "rm -rf ./jsdoc && find src -type f -name '*.js' -o -name '*.jsx'| xargs jsdoc -c ./conf.json",
    "flow:update": "flow-typed install",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:check": "flow check",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "npm-run-all --parallel flow:check lint test",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "author": "Eric Masiello",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "cheerio": "^0.22.0",
    "connect-flash": "^0.1.1",
    "cross-env": "^3.1.4",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "request": "^2.79.0",
    "synbydesign.design": "^0.3.1",
    "webpack": "^2.2.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "codecov": "^1.0.1",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "eslint": "^3.17.1",
    "eslint-config-ericmasiello": "1.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.38.0",
    "flow-typed": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-svg-sprite": "^1.3.6",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "regenerator-runtime": "^0.10.3",
    "style-loader": "^0.13.1",
    "svgo": "^0.7.2"
  }
}
