{
  "name": "@synbydesign/common-ui",
  "version": "0.1.0",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "scripts": {
    "test": "BABEL_ENV=test jest",
    "test:watch": "BABEL_ENV=test jest --watch",
    "lint": "run-p lint:script lint:style",
    "lint:fix": "eslint --ext=js,jsx . --cache --fix",
    "lint:script": "eslint . .storybook --ignore-pattern !.eslintrc.js --ignore-pattern !.babelrc.js --ignore-pattern !.stylelintrc.js",
    "lint:style": "stylelint 'src/**/*.scss'",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "browserslist": [
    "chrome >= 43",
    "firefox >= 45",
    "ie >= 11",
    "safari >= 6"
  ],
  "peerDependencies": {
    "prop-types": ">= 15.7.2",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
