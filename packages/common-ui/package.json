    "@storybook/react": "^5.2.1",
    "@storybook/storybook-deployer": "^2.8.1",
    "@svgr/rollup": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "autoprefixer": "^9.6.1",
